<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Security-sensitive data should not be backed up -->
        <exclude domain="sharedpref" path="security_settings.xml" />
        <exclude domain="database" path="security.db" />
        <exclude domain="file" path="native_libs/" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Also exclude from device transfer -->
        <exclude domain="sharedpref" path="security_settings.xml" />
        <exclude domain="database" path="security.db" />
        <exclude domain="file" path="native_libs/" />
    </device-transfer>
</data-extraction-rules>
